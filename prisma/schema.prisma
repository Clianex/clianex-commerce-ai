model UserModule {
  id        String   @id @default(cuid())
  userId    String
  moduleId  String
  active    Boolean  @default(false)
  createdAt DateTime @default(now())

  @@unique([userId, moduleId])
}
datasource db {
  provider = "postgresql"      // Puede ser "mysql", "sqlite", etc.
  url      = env("DATABASE_URL")  // Se obtiene desde la variable de entorno
}

